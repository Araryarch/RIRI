// Test: Array methods (map, filter, slice, etc.)
print("Test: JavaScript-like Array Methods");

let arr = [1, 2, 3, 4, 5];

// map
let doubled = arr.map((x) => x * 2);
print("Doubled:", doubled.at(0), doubled.at(1), doubled.at(2));

// filter
let evens = arr.filter((x) => x % 2 == 0);
print("Evens:", evens.at(0), evens.at(1));

// slice
let sliced = arr.slice(1, 3);
print("Sliced [1:3]:", sliced.at(0), sliced.at(1));

// indexOf
let idx = arr.indexOf(3);
print("Index of 3:", idx);

// includes
let has3 = arr.includes(3);
let has10 = arr.includes(10);
print("Has 3:", has3, "Has 10:", has10);

// join
let joined = arr.join(", ");
print("Joined:", joined);

// String methods
let str = "Hello World";
let lower = str.toLowerCase();
let upper = str.toUpperCase();
print("Lower:", lower);
print("Upper:", upper);

let parts = str.split(" ");
print("Split:", parts.at(0), parts.at(1));

print("ALL TESTS PASSED");
