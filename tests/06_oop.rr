print("Test 06: OOP");
let fail = 0;

class Counter {
    let count = 0;
    
    func inc() {
        this.count = this.count + 1;
    }
    
    func get() {
        return this.count;
    }
}

let c = new Counter();
c.inc();
c.inc();
if (c.get() != 2) { print("FAIL: class method/field"); fail = 1; }

if (fail == 0) { print("ALL TESTS PASSED"); } else { print("FAIL"); }
