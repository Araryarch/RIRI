// Arrays
let arr = [10, 20, 30];
console.log("Array first element:");
console.log(arr[0]);

// Math
console.log("Sin(0):");
console.log(Math.sin(0));
console.log("Cos(0):");
console.log(Math.cos(0));

// Switch
let day = 2;
console.log("Day 2 is:");
switch (day) {
    case 1: {
        console.log("Monday");
        break;
    }
    case 2: {
        console.log("Tuesday");
        break;
    }
    default: {
        console.log("Other");
    }
}
