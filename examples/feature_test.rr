// Arrays
let arr = [10, 20, 30];
print("Array first element:");
print(arr[0]);

// Math
print("Sin(0):");
print(Math.sin(0));
print("Cos(0):");
print(Math.cos(0));

// Switch
let day = 2;
print("Day 2 is:");
switch (day) {
    case 1: {
        print("Monday");
        break;
    }
    case 2: {
        print("Tuesday");
        break;
    }
    default: {
        print("Other");
    }
}
