console.log("--- BST Test ---");
let bst = new BST();
bst.insert(10);
bst.insert(5);
bst.insert(15);
bst.printInOrder();
if (bst.search(5)) {
    console.log("Found 5");
} else {
    console.log("5 not found");
}

console.log("--- AVL Test ---");
let avl = new AVL();
avl.insert(10);
avl.insert(20);
avl.insert(30);
avl.insert(40);
avl.insert(50);
avl.insert(25);
avl.printInOrder();

console.log("--- Heap Test ---");
let heap = new Heap();
heap.push(3);
heap.push(2);
heap.push(15);
heap.push(5);
heap.push(4);
heap.push(45);

console.log("Heap Top:");
console.log(heap.top());
console.log("Pop Heap:");
console.log(heap.pop());
console.log("New Top:");
console.log(heap.top());
