print("--- BST Test ---");
let bst = new BST();
bst.insert(10);
bst.insert(5);
bst.insert(15);
bst.printInOrder();
if (bst.search(5)) {
    print("Found 5");
} else {
    print("5 not found");
}

print("--- AVL Test ---");
let avl = new AVL();
avl.insert(10);
avl.insert(20);
avl.insert(30);
avl.insert(40);
avl.insert(50);
avl.insert(25);
avl.printInOrder();

print("--- Heap Test ---");
let heap = new Heap();
heap.push(3);
heap.push(2);
heap.push(15);
heap.push(5);
heap.push(4);
heap.push(45);

print("Heap Top:");
print(heap.top());
print("Pop Heap:");
print(heap.pop());
print("New Top:");
print(heap.top());
